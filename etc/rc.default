# Default values for rc.conf

hostname=amnesiac

ramd_enable=no
ramd_size=0
ramd_mount=/tmp
ramd_owner=root
ramd_group=wheel
ramd_mode=1777

inet_enable=no
inet_gateway=
inet_interfaces=

acct_enable=no
acct_file=/usr/adm/acct
acct_flags=
cron_enable=yes
inetd_enable=no
lpd_enable=no
named_enable=no
ntpd_enable=no
routed_enable=no
rwhod_enable=no
sendmail_enable=no
sendmail_flags=-q1h
syslogd_enable=yes
syslogd_flags=
